    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gjwnsdud2083.myapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "c29894c0-8069-4c04-adf0-0cb3966b09f5"
      }
    }
  }
}
